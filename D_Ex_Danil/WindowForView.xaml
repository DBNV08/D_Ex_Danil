<Window x:Class="D_Ex_Danil.WindowForView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:D_Ex_Danil"
        mc:Ignorable="d"
        Title="WindowForView" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Margin="15,15,0,0" Text="ООО Руль" VerticalAlignment="Top" HorizontalAlignment="Left" Width="200"/>
        <Button Margin="0,15,15,0" Content="Назад" Click="btn_Back_Click" VerticalAlignment="Top" HorizontalAlignment="Right" Width="200" />
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
